<Application
    x:Class="PAWB.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:PAWB.WPF"
    xmlns:viewmodels="clr-namespace:PAWB.WPF.ViewModels"
    xmlns:views="clr-namespace:PAWB.WPF.Views">

    <Application.Resources>

        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewmodels:LoginViewModel}">
                <views:LoginView/>
            </DataTemplate>
        </ResourceDictionary>

    </Application.Resources>
</Application>
